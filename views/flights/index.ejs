<%- include('../partials/header') %>
<br>
<h1>ALL DEPARTURE TIMES</h1>
<br>
<table>
 <thead>
   <tr>
     <th>Airline</th>
     <th>Airport</th>
     <th>Flight Number</th>
     <th>Departure</th>
   </tr>
 </thead>
 <tbody>
   <% flights.forEach(f => { %>
   <tr>
       <td><%= f.airline %></td>
       <td><%= f.airport %></td>
       <td><%= f.flightNo %></td>
       <td><%= f.departure.toLocaleDateString() %></td>
       <td><a id="details" href="/flights/<%= f._id %>">DETAILS</a></td> 
     </tr>
     <%  }) %>
 </tbody>
</table>

<%- include('../partials/footer') %>